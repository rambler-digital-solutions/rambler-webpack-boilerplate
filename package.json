{
  "name": "rambler-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Use it for simple and fast web development",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --progress --port 8090",
    "build": "rm -rf dist/* && NODE_ENV=production webpack -p --colors",
    "build-test": "node .bin/build-test",
    "test": "NODE_ENV=tdd npm run build-test",
    "test-ci": "NODE_ENV=test-ci testem ci"
  },
  "devDependencies": {
    "autoprefixer-core": "5.2.1",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.5.0",
    "babel-register": "^6.7.2",
    "chai": "3.5.0",
    "css-loader": "0.12.0",
    "eslint": "2.11.1",
    "eslint-config-rambler": "0.1.0",
    "eslint-loader": "1.3.0",
    "extract-text-webpack-plugin": "0.7.0",
    "file-loader": "0.8.1",
    "glob": "7.0.3",
    "html-webpack-plugin": "1.3.0",
    "jade": "1.11.0",
    "jade-loader": "0.7.1",
    "json-loader": "0.5.1",
    "lodash": "3.7.0",
    "manifest-revision-webpack-plugin": "0.1.0",
    "mocha": "2.4.5",
    "npmlog": "2.0.3",
    "path": "0.11.14",
    "phantomjs-prebuilt": "2.1.4",
    "postcss-loader": "0.6.0",
    "raw-loader": "0.5.1",
    "style-loader": "0.12.1",
    "stylint": "1.3.10",
    "stylint-loader": "1.0.0",
    "stylus-loader": "1.1.1",
    "testem": "1.6.0",
    "url-loader": "0.5.5",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rambler-digital-solutions/rambler-webpack-boilerplate"
  },
  "author": "Mike Chernobrov <m.chernobrov@rambler-co.ru> (http://rambler-co.ru)",
  "license": "MIT",
  "dependencies": {
    "backbone": "1.2.0",
    "fullpage.js": "2.6.5",
    "jquery": "2.1.4",
    "underscore": "1.8.3"
  }
}
