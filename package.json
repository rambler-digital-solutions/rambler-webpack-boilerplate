{
  "name": "rambler-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Use it for simple and fast web development",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --progress --port 8090",
    "build": "rm -rf dist/* && NODE_ENV=production webpack -p --colors",
    "build-test": "node .bin/build-test",
    "test": "NODE_ENV=tdd npm run build-test",
    "test-ci": "NODE_ENV=test-ci testem ci"
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "chai": "3.5.0",
    "css-loader": "0.26.1",
    "eslint": "3.15.0",
    "eslint-config-rambler": "0.2.6",
    "eslint-loader": "1.6.1",
    "eslint-plugin-react": "6.9.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "glob": "7.1.1",
    "html-webpack-plugin": "2.28.0",
    "json-loader": "0.5.4",
    "lodash": "4.17.4",
    "manifest-revision-webpack-plugin": "0.3.0",
    "mocha": "3.0.2",
    "npmlog": "4.0.2",
    "path": "0.12.7",
    "phantomjs-prebuilt": "2.1.11",
    "postcss-loader": "1.2.2",
    "pug": "2.0.0-beta5",
    "pug-loader": "2.3.0",
    "pug-walk": "1.1.1",
    "raw-loader": "0.5.1",
    "style-loader": "0.13.1",
    "stylint": "1.5.9",
    "stylint-loader": "1.0.0",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "testem": "1.10.3",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rambler-digital-solutions/rambler-webpack-boilerplate"
  },
  "author": "Mike Chernobrov <m.chernobrov@rambler-co.ru> (http://rambler-co.ru)",
  "license": "MIT",
  "dependencies": {
    "backbone": "1.3.3",
    "fullpage.js": "2.9.2",
    "jquery": "3.1.1",
    "underscore": "1.8.3"
  }
}
