{
  "name": "rambler-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Use it for simple and fast web development",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --progress --port 8090",
    "build": "rm -rf dist/* && NODE_ENV=production webpack -p --colors",
    "build-test": "node .bin/build-test",
    "test": "NODE_ENV=tdd npm run build-test",
    "test-ci": "NODE_ENV=test-ci testem ci"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "7.1.3",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.1.2",
    "css-loader": "0.28.7",
    "eslint": "4.6.1",
    "eslint-config-rambler": "0.3.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-react": "7.3.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "glob": "7.1.2",
    "html-webpack-plugin": "2.30.1",
    "json-loader": "0.5.7",
    "manifest-revision-webpack-plugin": "0.4.0",
    "mocha": "3.5.0",
    "npmlog": "4.1.2",
    "path": "0.12.7",
    "phantomjs-prebuilt": "2.1.15",
    "postcss-loader": "2.0.6",
    "pug": "2.0.0-rc.3",
    "pug-loader": "2.3.0",
    "pug-walk": "1.1.4",
    "raw-loader": "0.5.1",
    "stylint": "1.5.9",
    "stylint-loader": "1.0.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "testem": "1.18.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rambler-digital-solutions/rambler-webpack-boilerplate"
