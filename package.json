{
  "name": "rambler-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Use it for simple and fast web development",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --progress",
    "build": "rm -rf dist/* && NODE_ENV=production webpack -p --colors",
    "test:single": "rm -rf tmp/ && karma start karma.conf.js --single-run --browsers PhantomJS",
    "test:watch": "karma start karma.conf.js --browsers PhantomJS"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel-core": "^6.5.2",
    "babel-istanbul": "^0.6.0",
    "babel-istanbul-loader": "0.0.3",
    "babel-preset-es2015": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.12.0",
    "escope": "^3.4.0",
    "eslint-config-rambler": "git://github.com/rambler-digital-solutions/dotfiles.git",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^0.7.0",
    "file-loader": "^0.8.1",
    "html-webpack-plugin": "^1.3.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.7.1",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.1",
    "karma": "^0.13.21",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.7",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^1.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lodash": "^3.7.0",
    "manifest-revision-webpack-plugin": "0.0.2",
    "mocha": "^2.4.5",
    "path": "^0.11.14",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.6.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.12.1",
    "stylus-loader": "^1.1.1",
    "url-loader": "^0.5.5",
    "webpack": "^1.8.11",
    "webpack-dev-server": "^1.8.2"
